{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"rootDir": "./src-js",
		"outDir": "./dist",
		"moduleResolution": "node",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"declaration": true,
		"emitDeclarationOnly": true
	}
}
